{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "import": ["@cspell/dict-de-de/cspell-ext.json"],
    "ignoreWords": ["MartinThoma", "emph", "enquote"],
    "languageSettings": [],
    "overrides": [
        {
            "filename": "temp/MartinThoma/LaTeX-examples/**/*.{md,tex}",
            "dictionaries": ["latex"]
        },
        {
            "filename": ["temp/MartinThoma/LaTeX-examples/**/*"],
            "language": "en,de,lorem-ipsum",
            "languageSettings": [
                {
                    "locale": "de",
                    "caseSensitive": false
                }
            ]
        }
    ]
}
