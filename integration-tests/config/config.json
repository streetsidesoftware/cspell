{
  "repositories": [
    {
      "path": "w3c/specberus",
      "args": [
        "**/*.*"
      ],
      "url": "https://github.com/w3c/specberus.git"
    },
    {
      "path": "bitjson/typescript-starter",
      "args": [
        "{README.md,.github/*.md,src/**/*.ts}"
      ],
      "url": "https://github.com/bitjson/typescript-starter.git"
    },
    {
      "path": "liriliri/licia",
      "url": "https://github.com/liriliri/licia.git",
      "args": [
        "**/*.*",
        "*.*"
      ]
    },
    {
      "path": "wireapp/wire-webapp",
      "url": "https://github.com/wireapp/wire-webapp.git",
      "args": [
        "src/script/**/*.{js,ts,tsx,md}"
      ]
    },
    {
      "path": "typescript-cheatsheets/react",
      "url": "https://github.com/typescript-cheatsheets/react.git",
      "args": [
        "**/*.{ts,js,md}",
        "*.{ts,js,md}"
      ]
    },
    {
      "path": "graphql/express-graphql",
      "url": "https://github.com/graphql/express-graphql.git",
      "args": [
        "**/*.*",
        "*.*"
      ]
    },
    {
      "path": "wireapp/wire-desktop",
      "url": "https://github.com/wireapp/wire-desktop.git",
      "args": [
        "*.md",
        "electron/renderer/**/*.jsx",
        "electron/src/**/*.ts",
        "electron/html/*.html"
      ]
    },
    {
      "path": "graphql/graphql-relay-js",
      "url": "https://github.com/graphql/graphql-relay-js.git",
      "args": [
        "**/*.*",
        "*.*"
      ]
    },
    {
      "path": "aws-amplify/docs",
      "url": "https://github.com/aws-amplify/docs.git",
      "args": [
        "docs/**/*.md"
      ]
    },
    {
      "path": "webdeveric/webpack-assets-manifest",
      "url": "https://github.com/webdeveric/webpack-assets-manifest.git",
      "args": [
        "./{.github,src,test}/**/*.{js,json}",
        "./*.{md,js}",
        "./package.json"
      ]
    },
    {
      "path": "prettier/prettier",
      "url": "https://github.com/prettier/prettier.git",
      "args": [
        ".github/**/*",
        "**/*.*",
        "*.*"
      ]
    }
  ]
}
