{
  "name": "cspell-integration-tests",
  "private": true,
  "version": "0.1.0",
  "description": "Integration tests for cspell. It will execute cspell against various repositories and compare the results.",
  "bin": "tester.js",
  "scripts": {
    "build": "tsc -p .",
    "eslint": "node ../node_modules/eslint/bin/eslint",
    "lint": "npm run eslint -- --fix .",
    "lint-ci": "npm run eslint -- .",
    "postinstall": "npm run build",
    "integration-tests": "node tester.js check -f",
    "update-snapshots": "node tester.js check -u"
  },
  "author": "Jason Dent",
  "license": "MIT",
  "dependencies": {
    "commander": "^6.2.0",
    "jest-diff": "^26.6.1",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^14.14.2",
    "@types/shelljs": "^0.8.8",
    "jest": "^26.6.1",
    "typescript": "^4.0.3"
  }
}
