    "update-snapshots": "node tester.js check -u"
  },
  "author": "Jason Dent",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.2.0",
    "jest-diff": "^26.6.1",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@types/shelljs": "^0.8.8",
    "jest": "^26.6.3"
  },
  "main": "tester.js",
  "keywords": []
}
