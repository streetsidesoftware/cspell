{
  "name": "cspell-integration-tests",
  "private": true,
  "version": "5.2.4",
  "description": "Integration tests for cspell. It will execute cspell against various repositories and compare the results.",
  "bin": {
    "cspell-integration-tests": "tester.js"
  },
  "engines": {
    "node": ">10"
  },
  "scripts": {
    "build": "tsc -p .",
    "clean": "rimraf dist temp repositories/temp",
    "postinstall": "npm run build",
    "integration-tests": "node tester.js check -f",
    "test-jest": "jest",
    "update-snapshots": "node tester.js check -u"
  },
  "author": "Jason Dent",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^18.1.0",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "jest-diff": "^26.6.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "simple-git": "^2.34.2"
  },
  "devDependencies": {
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.25",
    "@types/rimraf": "^3.0.0",
    "@types/shelljs": "^0.8.8",
    "jest": "^26.6.3"
  },
  "main": "tester.js",
  "keywords": []
}
