{
  "name": "test-cspell-pipe",
  "version": "6.29.3",
  "description": "Pure testing package for @cspell/cspell-pipe. Its purpose is to verify @cspell/cspell-pipe dependencies.",
  "private": true,
  "type": "commonjs",
  "bin": "./bin.cjs",
  "scripts": {
    "clean": "shx rm -rf dist .tsbuildinfo",
    "build": "pnpm build:node10 && pnpm build:cjs",
    "build:node10": "tsc -p .",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean-build": "pnpm run clean && pnpm run build",
    "test": "pnpm test:unit && pnpm test:smoke",
    "test:smoke": "node ./bin.cjs",
    "test:unit": "jest"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cspell/cspell-pipe": "workspace:*"
  },
  "main": "index.js",
  "keywords": []
}
