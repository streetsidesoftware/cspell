{
  "name": "test-cspell-pipe-rollup",
  "version": "6.18.1",
  "description": "Pure testing package for @cspell/cspell-pipe. It purpose is to verify @cspell/cspell-pipe dependencies.",
  "private": true,
  "bin": "./bin.mjs",
  "browser": "./dist/browser.js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "scripts": {
    "clean": "shx rm -rf dist .tsbuildinfo",
    "build": "rollup -c rollup.config.mjs",
    "build-dev": "tsc -p tsconfig.dev.json",
    "clean-build": "pnpm run clean && pnpm run build",
    "compile": "tsc -p .",
    "test": "pnpm test:unit && pnpm test:smoke",
    "test:smoke": "node ./bin.mjs && node ./bin.cjs",
    "test:unit": "vitest run"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cspell/cspell-pipe": "workspace:*"
  },
  "keywords": [],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "rollup": "^3.9.1",
    "vitest": "^0.26.3"
  },
  "engines": {
    "node": ">=14"
  }
}
