{
  "name": "example-yarn",
  "version": "1.0.0",
  "private": true,
  "description": "CSpell Examples with Yarn",
  "scripts": {
    "preinstall": "pnpm setup-yarn",
    "setup-yarn": "pnpm setup-yarn:cspell-eslint-plugin",
    "setup-yarn:cspell-eslint-plugin": "cd cspell-eslint-plugin && corepack yarn install",
    "test": "pnpm test:cspell-eslint-plugin",
    "test:cspell-eslint-plugin": "cd cspell-eslint-plugin && corepack yarn test",
    "update-yarn": "pnpm update:cspell-eslint-plugin",
    "update:cspell-eslint-plugin": "cd cspell-eslint-plugin && corepack use yarn@stable && corepack yarn update:dep"
  },
