{
  "name": "cspell-tools",
  "version": "5.0.1-alpha.8",
  "description": "Tools to assist with the development of cSpell",
  "typings": "dist/index.d.ts",
  "bin": {
    "cspell-tools": "bin.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "clean-build": "npm run clean && npm run build",
    "clean": "rimraf dist temp",
    "eslint": "node ../../node_modules/eslint/bin/eslint",
    "lint": "npm run eslint -- --fix .",
    "lint-ci": "npm run eslint -- .",
    "coverage": "jest --coverage",
    "test-watch": "jest --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell.git"
  },
  "keywords": [
    "cSpell",
    "Word",
    "List",
    "Compiler"
  ],
  "files": [
    "bin.js",
    "dist/**",
    "!**/*.map",
    "!**/*.test.*"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell/labels/cspell-tools"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell#readme",
  "dependencies": {
    "commander": "^6.2.0",
    "cspell-io": "^5.0.1-alpha.8",
    "cspell-trie-lib": "^5.0.1-alpha.8",
    "cspell-util-bundle": "^5.0.1-alpha.8",
    "fs-extra": "^9.0.1",
    "gensequence": "^3.1.1",
    "glob": "^7.1.6",
    "hunspell-reader": "^3.2.0",
    "iconv-lite": "^0.6.2",
    "minimatch": "^3.0.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "jest": "^26.6.1",
    "rimraf": "^3.0.2"
  }
}
