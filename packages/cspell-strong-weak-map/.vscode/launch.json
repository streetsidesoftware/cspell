    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "cspell-strong-weak-map: Jest current-file",
            "program": "${workspaceFolder}/../../node_modules/jest/bin/jest.js",
            "args": ["--runInBand", "${fileBasename}"],
            "runtimeArgs": ["--expose-gc"],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "cspell-strong-weak-map: Jest All",
            "program": "${workspaceFolder}/../../node_modules/jest/bin/jest.js",
            "args": ["--runInBand"],
            "runtimeArgs": ["--expose-gc"],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}
