{
  "name": "@cspell/dictionary-bundler-plugin",
  "type": "module",
  "version": "9.6.4",
  "description": "A plugin for bundling cspell dictionaries.",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "private": true,
  "license": "MIT",
  "keywords": [
    "cspell",
    "dictionary",
    "bundler",
    "plugin"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./api": "./dist/api.mjs",
    "./esbuild": "./dist/esbuild.mjs",
    "./farm": "./dist/farm.mjs",
    "./rolldown": "./dist/rolldown.mjs",
    "./rollup": "./dist/rollup.mjs",
    "./rspack": "./dist/rspack.mjs",
    "./vite": "./dist/vite.mjs",
    "./webpack": "./dist/webpack.mjs",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/*.mjs",
    "dist/*.d.ts"
  ],
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "build:tsc": "tsc",
    "watch": "tsdown --watch",
    "test": "pnpm build:tsc && vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.3.0",
    "cspell-config-lib": "workspace:*",
    "cspell-trie-lib": "workspace:*",
    "unplugin": "^3.0.0"
  },
  "devDependencies": {
    "@cspell/cspell-types": "workspace:*",
    "@cspell/dict-html-symbol-entities": "^4.0.5",
    "@sxzz/test-utils": "^0.5.15",
    "magic-string": "^0.30.21",
    "vitest": "~4.0.18"
  }
}
