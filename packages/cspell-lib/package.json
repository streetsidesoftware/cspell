{
  "name": "cspell-lib",
  "version": "5.1.3",
  "description": "A library of useful functions used across various cspell tools.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!**/*.map",
    "config/**"
  ],
  "scripts": {
    "clean": "rimraf dist temp coverage",
    "build": "npm run compile && npm run build-schema",
    "build-schema": "ts-json-schema-generator --no-top-ref --path src/Settings/CSpellSettingsDef.ts --type CSpellSettings --validation-keywords markdownDescription  --validation-keywords scope --validation-keywords deprecationMessage -o  ../../cspell.schema.json",
    "clean-build": "npm run clean && npm run build",
    "compile": "tsc -p .",
    "watch": "tsc --watch -p .",
    "coverage": "jest --coverage",
    "test-watch": "jest --watch",
    "prepublishOnly": "npm run clean-build && npm test",
    "test": "jest",
    "update-snapshot": "jest --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell.git"
  },
  "keywords": [
    "spell",
    "checker",
    "code",
    "camel",
    "case",
    "spelling",
    "spell checker",
    "spelling checker",
    "lint"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell/labels/cspell-lib"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell#readme",
  "dependencies": {
    "@cspell/cspell-bundled-dicts": "^5.1.0",
    "comment-json": "^4.1.0",
    "configstore": "^5.0.1",
    "cspell-io": "^5.1.3",
    "cspell-trie-lib": "^5.1.3",
    "fs-extra": "^9.1.0",
    "gensequence": "^3.1.1",
    "minimatch": "^3.0.4",
    "resolve-from": "^5.0.0",
    "resolve-global": "^1.0.0",
    "vscode-uri": "^3.0.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@cspell/dict-cpp": "^1.1.37",
    "@cspell/dict-csharp": "^1.0.10",
    "@cspell/dict-css": "^1.0.10",
    "@cspell/dict-html": "^1.1.5",
    "@cspell/dict-nl-nl": "^1.0.34",
    "@cspell/dict-python": "^1.0.33",
    "@types/configstore": "^4.0.0",
    "@types/fs-extra": "^9.0.6",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^14.14.22",
    "cspell-dict-nl-nl": "^1.1.1",
    "jest": "^26.6.3",
    "lorem-ipsum": "^2.0.3",
    "rimraf": "^3.0.2",
    "ts-json-schema-generator": "^0.82.0"
  }
}
