{
  "name": "@cspell/cspell-dynamic-loader",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "version": "6.17.0",
  "description": "Load cspell or cspell-lib on demand",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "clean": "rimraf dist coverage .tsbuildinfo",
    "build": "pnpm run compile",
    "build-dev": "tsc -p tsconfig.dev.json",
    "clean-build": "pnpm run clean && pnpm run build",
    "compile": "tsc -p .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell.git"
  },
  "files": [
    "dist",
    "!**/__mocks__",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!**/*.map"
  ],
  "keywords": [
    "cspell",
    "cspell-loader",
    "types"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "cspell": "workspace:*",
    "cspell-lib": "workspace:*",
    "import-fresh": "^3.3.0"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  }
}
