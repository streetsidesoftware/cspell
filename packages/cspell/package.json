{
  "name": "cspell",
  "version": "4.2.8",
  "description": "A Spelling Checker for Code!",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "cspell": "bin.js"
  },
  "files": [
    "bin.js",
    "dist/**",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!**/*.map"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p .",
    "clean-build": "npm run clean && npm run build",
    "coverage": "jest --coverage",
    "prepublishOnly": "npm run clean-build && npm test",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell.git"
  },
  "keywords": [
    "spell",
    "checker",
    "code",
    "camel",
    "case",
    "spelling",
    "spell checker",
    "spelling checker",
    "lint"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^7.2.0",
    "comment-json": "^4.2.2",
    "cspell-glob": "^0.1.25",
    "cspell-lib": "^4.3.12",
    "fs-extra": "^10.1.0",
    "gensequence": "^3.1.1",
    "get-stdin": "^8.0.0",
    "glob": "^7.2.0",
    "minimatch": "^5.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
  }
}
