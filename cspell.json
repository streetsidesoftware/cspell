{
    "version": "0.1",
    "dictionaryDefinitions": [
        {
            "name": "workspace",
            "file": "./cspell-dict.txt",
            "description": "Custom Workspace Dictionary"
        }
    ],
    "dictionaries": ["workspace"],
    "ignorePaths": [
        "azure-pipelines.yml",
        "**/jest.config.js",
        "packages/cspell-lib/dictionaries/**",
        "packages/cspell-lib/migrated_dictionaries/**",
        "**/integration-tests/repositories/**",
        "**/integration-tests/snapshots/**",
        "package*.json",
        ".gitattributes",
        ".gitignore",
        "cspell.json",
        "packages/Samples/**",
        "packages/*/samples/**",
        "**/{cspell.json,cSpell.json,.cspell.json}",
        "**/package*.json",
        "**/temp/**/*",
        "**/node_modules/**",
        "**/.gitignore",
        "**/dist/**",
        "**/*.trie",
        "**/.vscode/**"
    ],
    "ignoreWords": [
        "xregexp",
        "lcov",
        "repo",
        "tscv",
        "tsdk"
    ]
}
